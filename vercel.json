{
  "version": 2,
  "routes": [
    {
      "src": "/education",
      "dest": "/education"
    },
    {
      "src": "/experience",
      "dest": "/experience"
    },
    {
      "src": "/projects",
      "dest": "/projects"
    },
    {
      "src": "/achievements",
      "dest": "/achievements"
    },
    {
      "src": "/thoughts",
      "dest": "/thoughts"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}